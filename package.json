{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/src/index.js",
    "dev": "ts-node-dev 'src/index.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "test:functional": "jest --projects ./test --forceExit --runInBand",
    "test:unit": "jest",
    "test": "npm run test:functional"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nelsonwenner/node-typescript-api.git"
  },
  "author": "Nelson Wenner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nelsonwenner/node-typescript-api/issues"
  },
  "homepage": "https://github.com/nelsonwenner/node-typescript-api#readme",
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "@types/bcrypt": "^3.0.0",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.10.14"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.10.0",
    "@types/node": "^14.14.7",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  }
}
